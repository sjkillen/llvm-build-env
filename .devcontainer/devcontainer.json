{
	"name": "Dev Dockerfile",
	"context": ".",
	"dockerFile": "../docker/Dockerfile",
	"settings": { 
		"terminal.integrated.shell.linux": null
	},
	"workspaceMount": "source=${localWorkspaceFolder},target=/volume,type=bind,consistency=cached",
	"workspaceFolder": "/volume",
	"extensions": []
}